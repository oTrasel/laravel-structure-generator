{
    "name": "trasel/laravel-structure-generator",
    "description": "Package to generate Services and Repositories or complete Repositories/Services/Controllers structure in Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Trasel\\StructureGenerator\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "version": "1.0.0",
    "extra": {
        "laravel": {
            "providers": [
                "Trasel\\StructureGenerator\\StructureGeneratorServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "echo 'Package installed successfully!'"
        ],
        "post-update-cmd": [
            "echo 'Package updated successfully!'"
        ]
    }
}
